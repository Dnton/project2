<br><br>
<div class="container-fluid well span6">
	<div class="row-fluid">
        <div class="span8">
            <h3>Welcome <%= user %></h3>
						<br>
            <h4>Your Existing Posts Are Below!</h4>
        </div>
</div>
</div>


<div class="container">
<div class="row">
    <div class="col-md-1"></div>
	<div class="col-md-6">
		<div id="postlist">
			<div class="panel">
                <div class="panel-heading">
                    <div class="text-center">
                        <div class="row">
                        </div>
                    </div>
                </div>

            <div class="panel-body">
              <a class="btn btn-primary btn-lg" href="/posts/new" role="button">New Post</a></div>
        </div>

				<div>
				           <% foundpost.forEach(function(post) {
				             %>
				             <div class="panel-footer">
				             <a href="/posts/<%= post.id %>"><h4> <%= post.header %> </h4> </a>

											<form method="POST" action="/posts/<%= post.id%>?_method=DELETE">
						          <input type="hidden" name="_method" value="DELETE">
						          <button class="label label-default" type="submit">Delete</button>
										</div>
										<br>
				            <% }) %>
				      </ul>
					</div>
    </div>
</div>

	<div class="col-md-1"></div>
	<div class="col-md-3">
	</div>
	<div class="col-md-1">
	</div>
</div>
</div>
